{
    "cSpell.language": "pt-BR,en",
    "cSpell.enableFiletypes": [
        "bat",
        "bibtex",
        "bibtex-style",
        "bsv",
        "clojure",
        "coffeescript",
        "diff",
        "dockerfile",
        "doctex",
        "fsharp",
        "log"
    ],
    "cSpell.enabled": false,
    "files.associations": {
        "array": "cpp",
        "*.tcc": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "memory": "cpp",
        "optional": "cpp",
        "string_view": "cpp",
        "fstream": "cpp",
        "istream": "cpp",
        "ostream": "cpp",
        "vector": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "tuple": "cpp"
    }
}